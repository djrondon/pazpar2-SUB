<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xsl/doc/pazpar2-docs.xsl"?>
<service
		xmlns="http://www.indexdata.com/pazpar2/1.0"
		xmlns:xi="http://www.w3.org/2001/XInclude"
		id="SUB">
<!--
	Targets used for SUB Homepage
	* GBV Databases, including Göttingen Opac, via new SRU service
	* Digizeitschriften from OAI Harvesting
	* GoeDoc

	2010-2012: Sven-S. Porst, SUB Göttingen <porst@sub.uni-goettingen.de>
-->

	<settings src="settings/gbv-sru-neu.xml"/>
	<settings>
		<set target="sru.gbv.de/olc" name="pz:name" value="Online Contents"/>
		<set target="sru.gbv.de/natliz" name="pz:name" value="Nationallizenzen Bücher"/>
		<set target="sru.gbv.de/natlizzss" name="pz:name" value="Nationallizenzen Zeitschriften"/>
	</settings>


	<settings src="settings/SUB-Harvesting-solr.xml"/>
	<settings>
		<set target="vlib.sub.uni-goettingen.de/solr/digizeitschriften" name="pz:name" value="DigiZeitschriften"/>
		<set target="vlib.sub.uni-goettingen.de/solr/digizeitschriften" name="pz:maxrecs" value="250"/>
	</settings>


	<settings src="settings/GoeScholar.xml"/>
	<settings src="settings/GEO-LEO-edocs.xml"/>



	<xi:include href="metadata/SUB-default.xml"/>

</service>
